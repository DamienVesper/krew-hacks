                <li class="nav-item" id="li-clan-chat" style="display:none;">
                    <a class="nav-link" id="chat-clan" href="#">Clan
                        <i id="clan-chat-alert" class="icofont icofont-speech-comments" style="display: none;"></i>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" id="chat-local" href="#">Local
                        <i id="local-chat-alert" class="icofont icofont-speech-comments" style="display: none;"></i>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link active" id="chat-global" href="#">Global
                        <i id="global-chat-alert" class="icofont icofont-speech-comments" style="display: none;"></i>
                    </a>
                </li>

                <span style="margin-right: 5px; margin-top: 5px; float: right;"><label id="hours">00</label>:<label id="minutes">00</label>:<label id="seconds">00</label></span>
            </ul>

            <div id="chat-history"></div>

            <div class="form-group">
                <input type="text" maxlength="60" id="chat-message" class="form-control" placeholder="message">
            </div>
        </div>

        <!-- Show Chat button -->
        <div id="show-chat" class="btn btn-primary" style="bottom: 0px; display: none; position: absolute; margin: 15px;">
            <i class="icofont icofont-rounded-up"></i>
            Show Chat
        </div>

        <!-- Inventory UI (Bottom Middle) -->
        <div id="experience-ui">
            <div class="text-center" style="margin-top: 2px;">
                <span class="text-danger">
                    <i class="icofont icofont-heart"></i>
                    <span class="ship-hp"></span> /
                    <span class="ship-max-hp"></span>
                </span>
                <span style="color: #65d94f!important;">
                    <i class="icofont icofont-speed-meter"></i>
                    <span class="ship-speed"></span>
                </span>
                <span class="text-info">
                    <i class="icofont icofont-people"></i>
                    <span class="ship-krew-count"></span> /
                    <span class="ship-max-capacity"></span>
                </span>
                <span class="text-warning">
                    <i class="icofont icofont-cube"></i>
                    <span class="ship-cargo"></span>
                </span>
            </div>

            <div id="inventory-container">
                <div data-tooltip="Press '1' to switch to cannon" data-tooltip-location="top" id="cannon-item-div" class="text-warning" style="font-size: 12px;background-color: #f9f9f9;border-radius: 5px;position: relative; border: 5px solid #f0ad4e">
                    <div style="position: absolute;top: 0px;">1</div>
                    <img src="/assets/img/tools/cannon.png" alt="" style="width: 36px; height: auto;">
                </div>
                <div data-tooltip="Press '2' to switch to fishing rod" data-tooltip-location="top" id="rod-item-div" class="text-warning" style="font-size: 12px; background-color: #f9f9f9; border-radius: 5px; position: relative">
                    <div style="position: absolute;top: 1px;">2</div>
                    <img src="/assets/img/tools/fishingrod.png" alt="" style="width: 36px;height: auto;">
                </div>
                <div data-tooltip="Press '3' to switch to spyglass" data-tooltip-location="top" id="spyglass-item-div" class="text-warning" style="font-size: 12px; background-color: #f9f9f9; border-radius: 5px; position: relative">
                    <div style="position: absolute;top: 1px;">3</div>
                    <img src="/assets/img/tools/spyglass.png" alt="" style="width: 36px;height: auto;">
                </div>
            </div>

            <div id="experience-attribute-container">
                <div data-tooltip="Press '5' to increase your fire rate" data-tooltip-location="top" class="experience-attribute-fireRate" title="Fire rate">
                    <div class="level-up-button" data-attribute="fireRate" style="display: none">
                        <i class="icofont icofont-caret-up"></i>
                        <div style="font-size:12px;position:absolute;top: 15px;left: 15px;" class="text-warning">5</div>
                    </div>
                    <i class="icofont icofont-time"></i>
                    <span>0</span>
                    <small>Fire rate</small>
                </div>
                <div data-tooltip="Press '6' to improve your distance" data-tooltip-location="top" class="experience-attribute-distance" title="distance">
                    <div class="level-up-button" data-attribute="distance" style="display: none">
                        <i class="icofont icofont-caret-up"></i>
                        <div style="font-size:12px;position:absolute;top: 15px;left: 15px;" class="text-warning">6</div>
                    </div>
                    <i class="icofont icofont-dart"></i>
                    <span>0</span>
                    <small>Distance</small>
                </div>
                <div data-tooltip="Press '7' to improve your damage" data-tooltip-location="top" class="experience-attribute-damage" title="Damage">
                    <div class="level-up-button" data-attribute="damage" style="display: none">
                        <i class="icofont icofont-caret-up"></i>
                        <div style="font-size:12px;position:absolute;top: 15px;left: 15px;" class="text-warning">7</div>
                    </div>
                    <i class="icofont icofont-fire-alt"></i>
                    <span>0</span>
                    <small>Damage</small>
                </div>
            </div>
            <div id="experience-bar" data-progress="" data-info="Level 0">
                <div style="width: 0%" role="progressbar"></div>
            </div>
        </div>

        <!-- Earn gold tip -->
        <div id="earn-gold" class="text-warning" style="display: none; font-size: 18px;background-color:rgba(0, 0, 0, 0.4); border-radius: 5px; padding: 5px;">Earn gold by catching crabs and shells!</div>
        <span id="delta-gold" class="h1"></span>

        <!-- Top Menu UI (Top Middle) -->
        <div id="top-menu-ui">
            <table style="table-layout: fixed; width: 100%; margin-top: 5px">
                <tr>
                    <td style="width: 12%;">
                        <div data-tooltip="Buy ships, items and goods" data-tooltip-location="bottom" id="toggle-shop-modal-button" class="btn btn-md disabled toggle-shop-modal-button">
                            <i class="icofont icofont-shopping-cart"></i>
                        </div>
                    </td>
                    <td style="width: 12%;">
                        <div data-tooltip="Join a krew" data-tooltip-location="bottom" id="toggle-krew-list-modal-button" class="btn btn-md disabled toggle-krew-list-modal-button">
                            <i class="icofont icofont-people"></i>
                        </div>
                    </td>
                    <td style="width: 12%;">
                        <div data-tooltip="Change krew name, manage krew members, lock your krew" data-tooltip-location="bottom" id="toggle-ship-status-button" class="btn btn-md toggle-ship-status-button">
                            <i class="icofont icofont-sail-boat-alt-2"></i>
                        </div>
                    </td>
                    <td style="width: 40%;">
                        <div style="font-size: 13px; margin-top: 5px" class="text-warning"><img id="gold-img" src="/assets/img/misc/gold.png" style="height: 20px; margin-bottom: 6px">
                            <span class="h5" style="margin-left: 2%;">
                                <span class="my-gold text-warning" id="gold" style="font-size: 1.5rem;">0</span>
                            </span>
                        </div>
                    </td>
                    <td style="width: 12%;">
                        <div data-tooltip="Modify your client's settings" data-tooltip-location="bottom" id="toggle-game-settings-button" class="btn btn-md toggle-game-settings-button">
                            <i class="icofont icofont-options"></i>
                        </div>
                    </td>
                    <td style="width: 12%;">
                        <div data-tooltip="Show player and krew quests" data-tooltip-location="bottom" id="toggle-quest-button" class="btn btn-md">
                            <i class="icofont icofont-book"></i>
                        </div>
                    </td>
                    <td style="width: 12%;">
                        <div data-tooltip="Show hotkeys" data-tooltip-location="bottom" id="toggle-help-button" class="btn btn-md">
                            <i class="icofont icofont-question"></i>
                        </div>
                    </td>
                </tr>
            </table>
        </div>

        <!-- Performance stats-->
        <div id="stats-wrapper">
            <div id="fps-wrapper"><span>0 FPS</span></div>
            <div id="ping-wrapper"><span>0 MS</span></div>
        </div>

        <!-- Docking info -->
        <div id="docking-modal" class="absolute-center initial" style="display: none;">
            <div id="you-are-docked-message">You are docked at
                <span class="port-name"></span>
            </div>
            <div id="docking-modal-button" class="btn btn-primary disabled btn-lg">
                <span id="docking-time">Docking...</span>
            </div>
            <div id="cancel-exit-button" class="btn btn-success btn-lg">
                <i class="icofont icofont-ship-wheel"></i>
                <span>Sail (c)</span>
            </div>
        </div>

        <!-- Departure info -->
        <div id="departure-modal" class="absolute-center" style="display: none;">
            <div id="cancel-departure-button" class="btn btn-primary disabled btn-lg">
                <i class="icofont icofont-ship-wheel"></i>
                <span></span>
            </div>
        </div>

        <!-- Center info  Container -->
        <div class="text-sm-center" style="position: absolute; left: 50%; padding: 25px">
            <div id="center-div" style="position: relative; left: -50%;">
            </div>
        </div>

        <!-- Shopping window -->
        <div class="text-sm-center" style="position: absolute; left: 50%; padding: 25px">
            <div id="show-shopping-modal-button" class="btn btn-primary" style="position: relative; left: -50%; z-index: 100;display: none" data-toggle="modal" data-target="#shopping-modal">
                Open Shopping Window
            </div>
            <div id="center-div" style="position: relative; left: -50%;">
            </div>
        </div>

        <!-- Map (Lower Right) -->
        <div id="minimap-container">
            <canvas id="minimap" width="200" height="200"></canvas>
        </div>
    </div>
    <!-- END Game UI -->

    <!-- END Gameplay HTML -->





